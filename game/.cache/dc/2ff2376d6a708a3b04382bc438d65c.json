,"column":16},"source":"src/scenes/LoadScene.js","original":{"line":59,"column":8}},{"name":"start","generated":{"line":94,"column":17},"source":"src/scenes/LoadScene.js","original":{"line":59,"column":19}},{"generated":{"line":94,"column":22},"source":"src/scenes/LoadScene.js","original":{"line":59,"column":8}},{"name":"CST","generated":{"line":94,"column":23},"source":"src/scenes/LoadScene.js","original":{"line":59,"column":25}},{"name":"SCENES","generated":{"line":94,"column":32},"source":"src/scenes/LoadScene.js","original":{"line":59,"column":29}},{"generated":{"line":94,"column":38},"source":"src/scenes/LoadScene.js","original":{"line":59,"column":25}},{"name":"MENU","generated":{"line":94,"column":39},"source":"src/scenes/LoadScene.js","original":{"line":59,"column":36}},{"generated":{"line":94,"column":43},"source":"src/scenes/LoadScene.js","original":{"line":59,"column":8}},{"generated":{"line":94,"column":45},"source":"src/scenes/LoadScene.js","original":{"line":59,"column":42}},{"generated":{"line":94,"column":67},"source":"src/scenes/LoadScene.js","original":{"line":59,"column":8}},{"generated":{"line":95,"column":0},"source":"src/scenes/LoadScene.js","original":{"line":60,"column":5}},{"name":"Phaser","generated":{"line":99,"column":2},"source":"src/scenes/LoadScene.js","original":{"line":4,"column":31}},{"generated":{"line":99,"column":8},"source":"src/scenes/LoadScene.js","original":{"line":4,"column":37}},{"name":"Scene","generated":{"line":99,"column":9},"source":"src/scenes/LoadScene.js","original":{"line":4,"column":38}}],"sources":{"src/scenes/LoadScene.js":"import { CST } from \"../CST\";\n// import { MenuScene } from \"./MenuScene\";\n\nexport class LoadScene extends Phaser.Scene {\n    constructor() {\n        super({\n            key: CST.SCENES.LOAD,\n        });\n    }\n    init() {}\n    preload() {\n        // load image, spritesheet, sound\n\n        this.load.image(\"mainbg\", \"./assets/mainbg.jpg\");\n        this.load.image(\"options_button.png\", \"./assets/options_button.png\");\n        this.load.image(\"play_button.png\", \"./assets/play_button.png\");\n        this.load.image(\"mushroom.png\", \"./assets/mushroom.png\");\n        this.load.spritesheet(\"cat.png\", \"./assets/cat.png\", {\n            frameHeight: 100,\n            frameWidth: 200,\n        });\n        this.load.spritesheet(\"purple.png\", \"./assets/purple.png\", {\n            frameHeight: 50,\n            frameWidth: 100,\n        });\n\n        // create loading bar\n\n        let loadingBar = this.add.graphics({\n            fillStyle: {\n                color: 0xff00ff, //white\n            },\n        });\n\n        //Loader Events - complete, progress\n\n        //simulate large load\n        for (let i = 0; i < 100; i++) {\n            this.load.spritesheet(\"cat\" + i, \"./assets/cat.png\", {\n                frameHeight: 150,\n                frameWidth: 200,\n            });\n        }\n\n        this.load.on(\"progress\", (percent) => {\n            loadingBar.fillRect(\n                0,\n                this.game.renderer.height / 2,\n                this.game.renderer.width * percent,\n                50\n            );\n            console.log(percent);\n        });\n        this.load.on(\"complete\", () => {\n            console.log(\"done\");\n        });\n    }\n    create() {\n        this.scene.start(CST.SCENES.MENU, \"hello from LoadScene\");\n    }\n}\n"},"lineCount":null}},"error":null,"hash":"14869a6e33f3fdf8bbf27fcb3d5b6ecc","cacheData":{"env":{}}}